    function vtgl(tgl){
        let y = tgl.getFullYear()
        let m = tgl.getMonth() + 1
        let d = tgl.getDate()
        if (m<10) m = '0'+m
        if (d<10) d = '0'+d
        return `${y}-${m}-${d}`
    }

