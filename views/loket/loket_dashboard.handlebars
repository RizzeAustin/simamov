<script id="dataTabel" type="text/x-handlebars-template">
    {{#each daftar}}  
        <tr>
            <td>nomer</td>
            <td>{{this.kodeUnit}}</td>
            <td>{{this.nomorTransaksi}}</td>
            <td>{{this.tanggal.pengajuan}}</td>
            <td>{{this.nilaiPengajuan}}</td>
            <td>{{this.detail}}</td>
            <td>{{this.posisi}}</td>
            <td>{{this.status}}</td>
            <td>catatan</td>
        </tr>
    {{/each}}
</script>

